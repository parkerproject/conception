<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>Conception | {{title}}</title>
        <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
        <!-- bootstrap 3.0.2 -->
        <link href="/conception/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <!-- font Awesome -->
        <link href="/conception/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
        <!-- Ionicons -->
        <link href="/conception/css/ionicons.min.css" rel="stylesheet" type="text/css" />

       <link href="/conception/css/iCheck/all.css" rel="stylesheet" type="text/css" />

        <!-- Theme style -->
        <link href="/conception/css/AdminLTE.css" rel="stylesheet" type="text/css" />

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
          <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="skin-blue">
        <!-- header logo: style can be found in header.less -->
        <header class="header">
            <a href="#" class="logo">
                <!-- Add the class icon to your logo image or logo icon to add the margining -->
                Conception events
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
            </nav>
        </header>
        <div class="wrapper row-offcanvas row-offcanvas-left">
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="left-side sidebar-offcanvas">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">
                    <!-- Sidebar user panel -->
                    <div class="user-panel">
                        <div class="pull-left image">
                            <img src="/conception//img/avatar3.png" class="img-circle" alt="User Image" />
                        </div>
                        <div class="pull-left info">
                            <p>Hello, Admin</p>

                            <a href="/admin/logout">logout</a>
                        </div>
                    </div>

                    <!-- sidebar menu: : style can be found in sidebar.less -->
                    <ul class="sidebar-menu">
                        <li>
                            <a href="/conception">
                                <i class="glyphicon glyphicon-list"></i> <span>Dashboard</span>
                            </a>
                        </li>
											  <li>
                            <a href="/conception/artists">
                                <i class="glyphicon glyphicon-list"></i> <span>Artists</span>
                            </a>
                        </li>

                    </ul>
                </section>
                <!-- /.sidebar -->
            </aside>

            <!-- Right side column. Contains the navbar and content of the page -->
            <aside class="right-side">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    <h1>
                        Artists on Conception
                        <small>manage artists from this page</small>
                    </h1>
                </section>

                <!-- Main content -->
                <section class="content">

                    <div class="col-md-12 event_json">
											
											<div class="col-xs-12">
                            <div class="box">

                                <div class="box-body table-responsive no-padding">
                                    <table class="table table-hover">
                                        <tbody><tr>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Status</th>
                                            
                                        </tr>
																				{{#each data}}
                                        <tr>
                                            <td><a class="artist_profile" href="#" data-artwork_1="{{artwork_1}}" 
																									 data-artwork_1="{{artwork_1}}" 
																									 data-artwork_2="{{artwork_2}}" 
																									 data-artwork_3="{{artwork_3}}" 
																									 data-photo="{{photo}}" 
																									 data-genre="{{genre}}"
																									 data-age="{{age}}" 
																									 data-email="{{email}}" 
																									 data-tickets="{{tickets}}" 
																									 data-events="{{events}}"
																									 data-url="{{url}}" 
																									 data-full_name="{{full_name}}" 
																									 data-reserved="{{activate}}" 
																									 data-approved="{{approved}}">{{full_name}}</a></td>
                                            <td>{{email}}</td>
                                            <td>{{{label}}}</td>
                                            </tr>
																				{{/each}}
                                           
                                    </tbody></table>
                                </div><!-- /.box-body -->
                            </div><!-- /.box -->
                        </div>
									  </div>


                </section><!-- /.content -->
            </aside><!-- /.right-side -->
        </div><!-- ./wrapper -->
			
			
			<div id="artistModal" class="modal fade">
				
				<div class="row user-menu-container square">
        <div class="col-md-12 user-details">
            <div class="row coralbg">
                <div class="col-md-6 no-pad">
                    <div class="user-pad">
                        <h3 class="name"></h3>
                        <h4 class="event"><i class="fa fa-check-circle-o" style="color: green;"></i> attending <em><strong></strong></em> </h4>
                        <h4 class="age">Age: <strong></strong></h4>
											<h4 class="email">Email: <strong></strong></h4>
											 <h4 class="genre">Genre: <strong></strong></h4>
											<h4 class="artwork">Artwork: <strong></strong></h4>
											 <h4 class="url">Website: <strong></strong></h4>
<!-- 											<div class="social-content">
											<a class="social"><i class="fa fa-facebook fa-2"></i></a>
											<a class="social"><i class="fa fa-twitter fa-2"></i></a>
											<a class="social"><i class="fa fa-instagram fa-2"></i></a>
											</div> -->

                       <div class="status"></div>
											 <div class="activate"></div>
                    </div>
                </div>
                <div class="col-md-6 no-pad">
                    <div class="user-image">
                        <img src="" class="img-responsive thumbnail">
                    </div>
                </div>
            </div>
            <div class="row overview">
                <div class="col-md-4 user-pad text-center">
                    <h3>TICKETS</h3>
                    <h4 class="total-tickets">15</h4>
                </div>
                <div class="col-md-4 user-pad text-center">
                    <h3>SOLD</h3>
                    <h4 class="sold">0</h4>
                </div>
                <div class="col-md-4 user-pad text-center">
                    <h3>REMAINING</h3>
                    <h4 class="remaining">15</h4>
                </div>
            </div>
        </div>

        </div></div>
    

			

        <!-- jQuery 2.0.2 -->
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
        <!-- Bootstrap -->
        <script src="/conception/js/bootstrap.min.js" type="text/javascript"></script>
        <script src="/conception/js/iCheck/icheck.js" type="text/javascript"></script>
 

        <!-- AdminLTE App -->
    
        <script src="/conception/js/app.js" type="text/javascript"></script>


    </body>
</html>
