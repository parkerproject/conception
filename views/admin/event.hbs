  <body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">

      <header class="main-header">
        <!-- Logo -->
        <a href="/admin/conception_new" class="logo">
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><b>Conception</b>Events</span>
        </a>

      </header>

      <!-- =============================================== -->

      <!-- Left side column. contains the sidebar -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Sidebar user panel -->
          <!-- search form -->

          <!-- /.search form -->
          <!-- sidebar menu: : style can be found in sidebar.less -->
          <ul class="sidebar-menu">
            <li class="header">MAIN NAVIGATION</li>
<!-- 						<li><a href="/admin/conception_new"><i class="fa fa-circle-o"></i> Dashboard</a></li>
						<li><a href="/admin/conception_new"><i class="fa fa-circle-o"></i> Inbox</a></li> -->
						<li><a href="/admin/conception_new"><i class="fa fa-circle-o"></i> Events</a></li>
<!-- 						<li><a href="/admin/conception_new"><i class="fa fa-circle-o"></i> Artists</a></li>
						<li><a href="/admin/conception_new"><i class="fa fa-circle-o"></i> Book an Artist</a></li> -->
          </ul>
        </section>
        <!-- /.sidebar -->
      </aside>

      <!-- =============================================== -->

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            {{title}}
            <small>{{start_date}}</small>
          </h1>
        </section>

        <!-- Main content -->
        <section class="content">

          <!-- Default box -->
          <div class="box">
            <div class="box-header with-border">
              <h3 class="box-title"></h3>
              <div class="box-tools pull-right">
                <button class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-minus"></i></button>
                <button class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove"><i class="fa fa-times"></i></button>
              </div>
            </div>

						  <div class="box">
                <div class="box-header">
                </div><!-- /.box-header -->

                <div class="box-body">
                  <table id="example1" class="table table-bordered table-hover" style="font-size:15px;">
                    <thead>
                      <tr>
                        <th>Artist_name</th>
                        <th>Genre</th>
                        <th>Booker</th>
                        <th>Approved</th>
											  <th>Status 2</th>
												<th>Notes</th>
                      </tr>
                    </thead>
                    <tbody>
                     {{#each sortedArtists}}
                      <tr>
                        <td><a href="/admin/{{../event_id}}/artist/{{user_token}}">{{full_name}}</a></td>
                        <td style="text-transform: capitalize;">{{genre}}</td>
                        <td>{{record.booker}}</td>
                        <td>{{approved}}</td>
                        <td>{{record.status_2}}</td>
												<td>{{record.notes}}</td>
                      </tr>
										 {{/each}}
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
              </div><!-- /.box -->
          </div><!-- /.box -->

        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->

      <footer class="main-footer">
        <div class="pull-right hidden-xs">
          <b>Version</b> 2.3.0
        </div>
				<strong>Copyright &copy; 2016 Conception Arts</strong>.
      </footer>


      <!-- Add the sidebar's background. This div must be placed
           immediately after the control sidebar -->
      <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->

    <!-- jQuery 2.1.4 -->
    <script src="/AdminLTE/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Bootstrap 3.3.5 -->
    <script src="/AdminLTE/bootstrap/js/bootstrap.min.js"></script>

		    <!-- DataTables -->
    <script src="/AdminLTE/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="/AdminLTE/plugins/datatables/dataTables.bootstrap.min.js"></script>

    <!-- SlimScroll -->
    <script src="/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="/AdminLTE/plugins/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/AdminLTE/dist/js/app.min.js"></script>


