                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav">
                            <li><a href="/about">about us</a></li>
                            <li><a href="/press">press</a></li>
<!--                             <li><a href="/campus">campus</a></li> -->
                            <li><a href="/what-new">what's new</a></li>
                            <li><a href="/community">community</a></li>
                        </ul>
                        <ul class="nav navbar-nav navbar-right social">
													<li><a href="https://www.pinterest.com/conceptionarts"><i class="fa fa-pinterest"></i></a></li>
												  <li><a href="https://www.instagram.com/conceptionnyc"><i class="fa fa-instagram"></i></a></li>
                            <li><a href="https://www.facebook.com/ConceptionEvents"><i class="fa fa-facebook"></i></a></li>
                            <li class="twitt">
                                <a href="https://twitter.com/ConceptionNYC"><i class="fa fa-twitter"></i></a>
                            </li>

											</ul>
                    </div>
                    <!-- /.navbar-collapse -->
                </div>
                <!-- /.container -->
            </nav>
        </div>
    </header>


    <section class="">
        <div class="container text">
					  <div class="row">
                <div class="col-md-12">
                    <img src="{{logo}}" alt="" class="header_image">
                </div>
            </div>
					<div class="row">
					<div class="col-md-12 text-center underline">
               <h1 class="event-title">{{liveEvent.title}}</h1>
							<p><i class="month">{{month}}</i> <i class="day">{{day}}</i>
								<span>{{liveEvent.venue.address}} // {{start_time}} - {{end_time}}</span>
								<span class="event-location">{{liveEvent.venue.name}}</span>
							</p>
							 <p>{{{liveEvent.description}}}</p>
							<a href="{{buyUrl}}" class="btn btn-success btn-lg alert general">Buy Tickets</a>
            </div>
					</div>

						<div class="row">
							{{#each artists}}
		          <div class="col-lg-3 col-sm-6">

            <div class="card hovercard">
  <div class="cardheader" style="background:url(http://www.conceptionevents.com/artists_images/);">


                </div>
                <div class="avatar">
									{{#if photo}}
									<img src="https://artistworks.s3-us-west-2.amazonaws.com/artists_images/{{photo}}?dim=150x104" alt="{{full_name}}" />
									{{else}}
									<img src="http://www.conceptionevents.com/images/no-image.jpg?dim=150x104" alt="{{full_name}}" />
									{{/if}}
                </div>
                <div class="info">
                    <div class="title">
                        <a href = "/artist/{{user_token}}" class="truncate">{{full_name}}</a>
                    </div>
                    <div class="desc" style="text-transform:uppercase;">{{genre}}</div>
                </div>
                <div class="bottom">
									{{#if twitter_url}}
                    <a class="btn btn-card btn-info btn-twitter btn-sm" href="{{twitter_url}}">
                        <i class="fa fa-twitter"></i>
                    </a>
									{{/if}}
									{{#if facebook_url}}
                    <a class="btn btn-card btn-primary btn-sm" rel="publisher"
                       href="{{facebook_url}}">
                        <i class="fa fa-facebook"></i>
                    </a>
									{{/if}}
									{{#if instagram}}
                    <a class="btn btn-card btn-warning btn-sm" rel="publisher" href="{{instagram}}">
                        <i class="fa fa-instagram"></i>
                    </a>
									{{/if}}
									{{#if url}}
                    <a class="btn btn-card btn-default btn-sm" rel="publisher" href="{{url}}">
                        <i class="fa fa-external-link" style="color:gray"></i>
                    </a>
									{{/if}}
                </div>
            </div>

        </div>
       {{/each}}
	</div>





        </div>
    </section>
