    </header>

    <section class="">
        <div class="container text">


					<div class="row">
                <div class="col-md-3 col-sm-3">

                    <div class="user-wrapper">
                        <img src="https://artistworks.s3-us-west-2.amazonaws.com/artists_images/{{data.photo}}" class="img-responsive">
                    <div class="description">
                       <h4> {{data.full_name}}</h4>
                        <h5 style="text-transform: uppercase;"> <strong> {{data.genre}} </strong></h5>
						                <div class="bottom">
									{{#if data.twitter_url}}
                    <a class="btn btn-card btn-info btn-twitter btn-sm" href="{{data.twitter_url}}">
                        <i class="fa fa-twitter"></i>
                    </a>
									{{/if}}
									{{#if data.facebook_url}}
                    <a class="btn btn-card btn-primary btn-sm" rel="publisher"
                       href="{{data.facebook_url}}">
                        <i class="fa fa-facebook"></i>
                    </a>
									{{/if}}
									{{#if data.instagram}}
                    <a class="btn btn-card btn-warning btn-sm" rel="publisher" href="{{data.instagram}}">
                        <i class="fa fa-instagram"></i>
                    </a>
									{{/if}}
									{{#if data.url}}
                    <a class="btn btn-card btn-default btn-sm" rel="publisher" href="{{data.url}}">
                        <i class="fa fa-external-link" style="color:gray"></i>
                    </a>
									{{/if}}
                </div>
                        <hr>
											 {{#if turnOnTicketButton}}
												<a style="color:white;" href="{{buyUrl}}" class="btn btn-success">
													<i class="icon-ticket"></i>
													BUY TICKETS
												</a>
											{{/if}}
                    </div>
                     </div>
                </div>

                <div class="col-md-9 col-sm-9  user-wrapper">
                    <div class="description">
                         <h3> My Story: </h3>
                    <hr>
                     {{data.story}}
                    <h3> Artwork: </h3>
                    <hr>
										{{#if data.artwork_1}}
										<div class="row grid">
												<div class="col-xs-7 thumbnail">
														<img src="https://artistworks.s3-us-west-2.amazonaws.com/artists_images/{{data.artwork_1}}" />
												</div>
									   </div>
											{{/if}}
										{{#if data.artwork_2}}
										<div class="row grid">
												<div class="col-xs-7 thumbnail">
														<img src="https://artistworks.s3-us-west-2.amazonaws.com/artists_images/{{data.artwork_2}}" />
												</div>
									   </div>
											{{/if}}

											{{#if data.artwork_3}}
											<div class="row grid">
												<div class="col-xs-7 thumbnail">
														<img src="https://artistworks.s3-us-west-2.amazonaws.com/artists_images/{{data.artwork_3}}" />
												</div>
									     </div>
											{{/if}}
                    </div>
                </div>
            </div>





        </div>
    </section>
