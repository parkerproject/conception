  <!-- featured artist section -->
    <div class="row body-content">
    <div class="large-12 columns organizer-row">
		{{#if artists}}
    <h3>featured artists</h3>
		{{/if}}
    <div class="row artists-holder">
        {{#each artists}}
        <div class="large-4 columns featured_artists">
<a class="featured_artists_img" href="/artist/{{user_token}}">
   <img src="http://www.conceptionevents.com/artists_images/{{photo}}" alt="{{full_name}}" />
</a>
<span class = "name"><a href = "/artist/{{user_token}}">{{full_name}}
    <i class = "icon-angle-double-right"></i></a></span>
<span class="title">{{genre}}</span >
       </div>
       {{/each}}
    </div>
    </div>
  </div>

  <div class="row">
    <div class="large-12 columns">
      <span class="scrollup"><i class="icon-angle-double-up"></i>Scroll up</span>
    </div>
  </div>

  <div class="row">
    <div class="large-12 colums">
      <ul class="inline-list footer-social">
        <li>
          <a href="https://www.facebook.com/ConceptionEvents" class="icon-facebook-circled"></a>
