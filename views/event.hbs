<body id="event">
<div class="top-header-bar">
    <div class="row">
      <a href="/" class="large-7 columns logo">conception events</a>
     <a href="/login" class="large-1 columns login">Login</a>
      <div class="large-4 columns">
        <a href="/register">
            <button class="small-12"><span class="ti-plus"></span>
          <span class="text">join the experience<i>submit today!</i></span>
        </button>
        </a>
      </div>
    </div>
  </div>




  <div class="row">
    <div class="large-12 columns clearfix">
      <nav class="top-bar" data-topbar>
        <ul class="title-area">
          <li class="name">
            <!-- <h1><a href="#">My Site</a></h1> -->
          </li>
          <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
          <li class="toggle-topbar menu-icon"><a href="#"><span></span></a>
          </li>
        </ul>

        <section class="top-bar-section">
          <!-- Left Nav Section -->
          <ul class="left">
            <li class="events"><a href="/events">events</a></li>
            <li class="about_us"><a href="/about">about us</a></li>
            <li class="press"><a href="/press">press</a></li>
            <li class="gallery"><a href="/gallery">gallery</a></li>
            <li class="campus"><a href="/campus">campus</a></li>
            <li class="whats_new"><a href="/what-new">what's new</a></li>
          </ul>
        </section>
      </nav>
    </div>
  </div>

<div class="event-bg">
  <img src="" alt="" class="event-img">

  <div class="row event-header">
    <div class="large-12 columns">
        <div class="inner-details">
        <span class="event-tag">conception <i>emerging artists</i></span>
        <h1 class="event-title"></h1>
        <div class="row">
            <div class="large-3 columns month-day">
                <span>
                <i class="month">sep</i>
                <i class="day">19</i>
                </span>

            </div>
            <div class="large-5 columns left when-where"></div>
        </div>

        <div class="row">
            <div class="large-10 event-blurb">
                <p></p>
            </div>
        </div>
    </div>

    </div>
</div>
</div>

